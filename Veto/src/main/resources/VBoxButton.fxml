<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt4p1ae1.veto.ControllerVbox">
    <children>
        <Button fx:id="btn_home" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionHomeBTN" prefHeight="75.0" prefWidth="75.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="95.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/maison.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="btn_agenda" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionAgendaBTN" prefHeight="75.0" prefWidth="75.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="63.0" fitWidth="115.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/calendrier.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="btn_client" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionClientBTN" prefHeight="75.0" prefWidth="81.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="58.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/carnetAdresse.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="btn_animaux" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionAnimauxBTN" prefHeight="75.0" prefWidth="75.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="129.0" fitWidth="62.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/dossier.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="btn_Ordonnance" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionOrdonnanceBTN" prefHeight="75.0" prefWidth="75.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="63.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/docteur.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="btn_facture" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionStockBTN" prefHeight="75.0" prefWidth="82.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="65.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/justice.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="btn_log" ellipsisString="" graphicTextGap="0.0" mnemonicParsing="false" onAction="#onActionLogBTN" prefHeight="70.0" prefWidth="75.0" text="Calendrier">
            <graphic>
                <ImageView fitHeight="62.0" fitWidth="63.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/graphe.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
    <padding>
        <Insets top="10.0" />
    </padding>
</VBox>
