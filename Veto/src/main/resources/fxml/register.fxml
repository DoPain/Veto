<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt4p1ae1.veto.ConnectionMenu.Register.RegisterController">
    <AnchorPane id="rootPane" VBox.vgrow="ALWAYS">
        <TextField id="loginField" fx:id="loginField" layoutX="277.0" layoutY="138.0" promptText="Enter a login ..." />
        <Label layoutX="314.0" layoutY="79.0" text="Register">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
        <Label layoutX="186.0" layoutY="142.0" text="Login :">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <TextField id="passwordField" fx:id="passwordField" layoutX="277.0" layoutY="197.0" promptText="Enter a password ..." />
        <Label layoutX="156.0" layoutY="201.0" text="Password :">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Button id="registerButton" fx:id="registerButton" layoutX="375.0" layoutY="257.0" mnemonicParsing="false" onAction="#registerButtonPushed" text="Register" />
    </AnchorPane>
</VBox>
