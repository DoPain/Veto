<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pt4p1ae1.veto.Entity.Ville" table="Personne" catalog="veto">
        <id name="id" type="java.lang.Integer">
            <column name="idVille" />
            <generator class="identity" />
        </id>
        <property name="departement" type="java.lang.String">
            <column name="ville_departement" length="10" not-null="true" unique="true" />
        </property>
        <property name="nom" type="java.lang.String">
            <column name="ville_nom_simple" length="10" not-null="true" unique="false" />
        </property>
        <property name="codePostale" type="java.lang.String">
            <column name="ville_code_postal" length="10" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>
