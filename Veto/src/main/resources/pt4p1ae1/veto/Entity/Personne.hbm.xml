<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pt4p1ae1.veto.Entity.Personne" table="Personne" catalog="veto">
        <id name="id" type="java.lang.Integer">
            <column name="idPersonne" />
            <generator class="identity" />
        </id>
        <property name="nom" type="string">
            <column name="STOCK_CODE" length="10" not-null="true" unique="false" />
        </property>
        <property name="prenom" type="string">
            <column name="STOCK_CODE" length="10" not-null="true" unique="false" />
        </property>
        <property name="dateNaissance" type="java.util.Date">
            <column name="dateNaissance" length="10" not-null="false" unique="false" />
        </property>
        <property name="adresse" type="string">
            <column name="adresse" length="10" not-null="true" unique="false" />
        </property>
        <property name="mail" type="string">
            <column name="mail" length="10" not-null="true" unique="false" />
        </property>
        <property name="telephone" type="string">
            <column name="telephone" length="10" not-null="true" unique="false" />
        </property>

    </class>
</hibernate-mapping>
